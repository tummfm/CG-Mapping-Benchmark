<cg>
  <!-- fmatch section -->
  <fmatch>
    <!-- use constraint LS solver -->
    <constrainedLS>true</constrainedLS>
    <!-- Number of frames for block averaging -->
    <frames_per_block>20000</frames_per_block>
  </fmatch>
  
  <bonded>
    <!-- name of the interaction -->
    <name>bond</name>
    <!-- fmatch specific stuff -->
    <min>0.0</min>
    <max>1.0</max>
	<step>0.001</step>
    <fmatch>
        <min>0.14</min>
        <max>0.165</max>
      <step>0.005</step>
      <out_step>0.001</out_step>
    </fmatch>
  </bonded>
  <bonded>
    <!-- name of the interaction -->
    <name>angle</name>
    <!-- fmatch specific stuff -->
    <min>0</min>
    <max>1.0</max>
	<step>0.01</step>
    <fmatch>
      <min>1.8</min>
      <max>2.2</max>
      <step>0.05</step>
      <out_step>0.01</out_step>
    </fmatch>
  </bonded>
  
  <bonded>
    <name>dihedral</name>
    <periodic>1</periodic>
    <min>-3.14</min>
    <max>3.14</max>
    <step>0.314</step>
      <fmatch>
        <min>-3.14</min>
        <max>3.14</max>
        <step>0.314</step>
        <out_step>0.157</out_step>
      </fmatch>
  </bonded>
  
  <!-- example for a non-bonded interaction entry -->
  <non-bonded>
    <name>A-A</name>
    <type1>A</type1>
    <type2>A</type2>
    <min>0</min>
    <max>1.3</max>
    <step>0.01</step>
    <fmatch>
      <min>0.34</min>
      <max>1.0</max>
      <step>0.02</step>
      <out_step>0.01</out_step>
    </fmatch>
  </non-bonded>

  <non-bonded>
    <name>B-B</name>
    <type1>B</type1>
    <type2>B</type2>
    <min>0</min>
    <max>1.3</max>
    <step>0.01</step>
    <fmatch>
      <min>0.34</min>
      <max>1.0</max>
      <step>0.02</step>
      <out_step>0.01</out_step>
    </fmatch>
  </non-bonded>
  
  <non-bonded>
    <name>A-B</name>
    <type1>A</type1>
    <type2>B</type2>
    <min>0</min>
    <max>1.3</max>
    <step>0.01</step>
    <fmatch>
      <min>0.34</min>
      <max>1.0</max>
      <step>0.02</step>
      <out_step>0.01</out_step>
    </fmatch>
  </non-bonded>
  
  <!-- settings for gromacs .xvg table -->
  <inverse>
  <method>force-matching</method>  <!-- or ibi, imc, etc. -->
    <gromacs>
      <table_bins>0.001</table_bins>
      <pot_max>1000000</pot_max>
      <table_end>3.0</table_end>
    </gromacs>
  </inverse>
</cg>
